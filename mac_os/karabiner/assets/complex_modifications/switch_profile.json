{
    "title": "Switch profile",
    "rules": [
        {
            "description": "Switch to MacBook build-in keyboard profile",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "mandatory": [
                                "control",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'MacBook build-in keyboard'"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Switch to Corne Cherry profile",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "control",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'Corne Cherry'"
                        }
                    ]
                }
            ]
        }
    ]
}