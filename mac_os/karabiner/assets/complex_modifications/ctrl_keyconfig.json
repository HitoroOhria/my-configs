{
    "title": "Ctrl keyconfig",
    "rules": [
        {
            "available_since": "12.3.0",
            "description": "Ctrl + { / } to Cmd + Shift + { / }.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "open_bracket",
                            "modifiers": [
                                "command",
                                "shift"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "close_bracket",
                            "modifiers": [
                                "command",
                                "shift"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "Ctrl + M to Return.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "shift",
                                "option",
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "Ctrl + F / B to Right / Left.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "b",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "Ctrl + N / P to Down / Up.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "Ctrl + H / D to Delete / Delete forward .",
            "manipulators": [
                {
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "Ctrl + U to deletes to beginning of line",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": ["shift", "command"]
                        },
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                }
            ]
        }
    ]
}