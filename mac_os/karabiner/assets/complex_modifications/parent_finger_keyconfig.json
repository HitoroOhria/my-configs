{
    "title": "Parent finger keyconfig.",
    "rules": [
        {
            "description": "[parent] Ctrl to Eisuu if alone.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_control",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "parameters": {
                        "basic.to_if_held_down_threshold_milliseconds": 100
                    },
                    "to": [
                        {
                            "key_code": "left_control",
                            "lazy": true
                        }
                    ],
                    "to_if_held_down": [
                        {
                            "key_code": "left_control"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "japanese_eisuu"
                        }
                    ]
                }
            ]
        },
        {
            "description": "[parent] Fn to Kana if alone.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "fn",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "parameters": {
                        "basic.to_if_held_down_threshold_milliseconds": 100
                    },
                    "to": [
                        {
                            "key_code": "fn",
                            "lazy": true
                        }
                    ],
                    "to_if_held_down": [
                        {
                            "key_code": "fn"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "japanese_kana"
                        }
                    ]
                }
            ]
        },
        {
            "description": "[parent] Ctrl + fn + M to Cmd + Return",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": ["control", "fn"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter",
                            "modifiers": "command"
                        }
                    ]
                }
            ]
        },
        {
            "description": "[parent] Ctrl + fn + <F | B | P | N> to Option + <Right | Left | Up | Down> (NOTE: Place above [ctrl])",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": ["control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": "option"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "b",
                        "modifiers": {
                            "mandatory": ["control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": "option"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": ["control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow",
                            "modifiers": "option"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow",
                            "modifiers": "option"
                        }
                    ]
                }
            ]
        },
        {
            "description": "[parent] Ctrl + fn + <H | D> to Option + <Delete | Delete forward>",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": ["control", "fn"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace",
                            "modifiers": "option"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": ["control", "fn"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward",
                            "modifiers": "option"
                        }
                    ]
                }
            ]
        },
        {
            "available_since": "12.3.0",
            "description": "[parent] SandS",
            "manipulators": [
                {
                    "from": {
                        "key_code": "spacebar",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_shift"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "spacebar"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}